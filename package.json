{
  "name": "virtuous-vscode",
  "displayName": "Virtuous",
  "description": "The JSON Formatter that implements the Kary Coding Standards",
  "version": "0.0.1",
  "publisher": "karyfoundation",
	"repository": "http://codes.kary.us/coding-standards/formatters/virtuous/viscose",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "@kary/virtuous": "^1.0.0"
  }
}
